{
  "name": "newapp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "web/newapp/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/web/newapp",
        "index": "web/newapp/src/index.html",
        "baseHref": "/",
        "main": "web/newapp/src/main.tsx",
        "tsConfig": "web/newapp/tsconfig.app.json",
        "assets": [
          "web/newapp/src/favicon.ico",
          "web/newapp/src/assets"
        ],
        "styles": ["web/newapp/src/styles.scss"],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "web/newapp/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "web/newapp/src/environments/environment.ts",
              "with": "web/newapp/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "newapp:build",
        "hmr": true,
        "port": 4444
      },
      "configurations": {
        "development": {
          "buildTarget": "newapp:build:development"
        },
        "production": {
          "buildTarget": "newapp:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "web/newapp/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "newapp:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "web/newapp/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
